<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jzfw.dao.extend.BaseConfigExtendMapper">
	<select id="findByKey" resultType="com.jzfw.bean.BaseConfig">
		SELECT *
		FROM base_config
		where name = #{name}
	</select>

</mapper>